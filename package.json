{
  "name": "@tegonal/qwik-speak-deepl",
  "repository": "https://github.com/tegonal/qwik-speak-deepl",
  "description": "Qwik Speak Deepl Translator",
  "author": "Oliver Studer <oliver.studer@tegonal.com>",
  "license": "MIT",
  "version": "1.0.4",
  "type": "module",
  "main": "./dist/index.js",
  "bin": "./dist/qwik-speak-deepl.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "up": "yarn upgrade-interactive",
    "self-update": "yarn set version latest && yarn",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --fix --ext .ts src",
    "prettier": "prettier --write \"src/**/*.{ts}\"",
    "prettier:check": "prettier --check \"src/**/*.{ts}\"",
    "cleaner": "git add . && git clean -xdf && yarn"
  },
  "keywords": [
    "qwik",
    "qwik-speak",
    "qwik-speak-deepl",
    "deepl",
    "translator"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.16",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "commander": "^11.1.0",
    "deepl-node": "^1.11.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "locale-matcher": "^2.0.0",
    "lodash-es": "^4.17.21",
    "prettier": "^3.2.4",
    "rollup": "^4.9.6",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-command-line-args": "^2.5.1",
    "typescript": "^5.3.3"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": true
  },
  "packageManager": "yarn@4.1.0"
}
